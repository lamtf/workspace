or arg.type.name.equalsIgnoreCase('PictureType') then 
cname + '.getInt("'+arg.getName()+'")'
else if arg.type.name.equalsIgnoreCase('Money') then 
'Float.valueOf('+cname+'.getString("'+arg.getName()+'"))'
