		db.execSQL(CREATE_TABLE_COOKIE);
		db.execSQL(CREATE_TABLE_UPDATE_HISTORY);
		db.execSQL(CREATE_INDEX_UPDATE_HISTORY_TNAME);

[for (modul : Package | get_modules(root))]
		[name.ToCamelCase() /]Design.create(db);
[/for]

		fill(db);

	}

	@Override
